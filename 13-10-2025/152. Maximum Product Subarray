class Solution {
public:
    int maxProduct(vector<int>& nums) {

        int maximum=INT_MIN;
        int n=nums.size();

        for(int i=0;i<n;i++)
        {
            int mul=1;
            for(int j=i;j<n;j++)
            {
                mul=mul*nums[j];
                maximum=max(maximum,mul);
            }
            
        }
        return maximum;
    }
};
