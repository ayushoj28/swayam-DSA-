class Solution {
public:
    void rotate(vector<vector<int>>& matrix) {

        int temp;
        int row=matrix.size();
        int col=matrix[0].size();

        //transpose

        for(int i=0;i<row;i++)
        {
            for(int j=i+1;j<col;j++)
            {

                swap(matrix[i][j],matrix[j][i]);

            }
        }

        //reverse

        for(int j=0;j<row/2;j++)
        {
            for(int i=0;i<row;i++)
            {
                swap(matrix[i][j],matrix[i][row-1-j]);
            }
        }

        
    }
};
